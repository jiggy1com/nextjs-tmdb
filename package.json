{
  "name": "tmdb-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "graphql-codegen generate:resolvers generate:schemas",
    "dev": "concurrently \"npm:generate:watch\" \"npm:watch:resolvers\" \"npm:watch:schemas\" \"next dev\"",
    "dev-off": "concurrently \"npm:generate:watch\" \"next dev\"",
    "dev-old": "next dev",
    "build": "next build",
    "prebuild": "npm run generate && npm run generate:resolvers && npm run generate:schemas",
    "prebuild-old": "graphql-codegen generate:resolvers generate:schemas",
    "start": "next start",
    "lint": "eslint",
    "generate": "graphql-codegen",
    "generate:watch": "graphql-codegen --watch",
    "generate:resolvers": "node scripts/generate-resolvers.js",
    "watch:resolvers": "nodemon --watch app/api/graphql/resolvers --ext ts --exec \"npm run generate:resolvers\"",
    "generate:schemas": "node scripts/generate-schemas.js",
    "watch:schemas": "nodemon --watch app/api/graphql/schemas --ext graphql --exec \"npm run generate:schemas\""
  },
  "dependencies": {
    "@apollo/client": "^4.0.11",
    "@apollo/client-integration-nextjs": "^0.14.4",
    "@apollo/datasource-rest": "^6.4.1",
    "@apollo/server": "^5.2.0",
    "@as-integrations/next": "^4.1.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.7",
    "@graphql-tools/schema": "^10.0.31",
    "@graphql-typed-document-node/core": "^3.2.0",
    "client-integration-nextjs]": "link:@apollo/client-integration-nextjs]",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "next": "16.1.1",
    "nodemon": "^3.1.11",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sass": "^1.97.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typed-document-node": "^6.1.5",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-document-nodes": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-resolvers": "^5.1.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "glob": "^13.0.0",
    "prettier": "3.7.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
